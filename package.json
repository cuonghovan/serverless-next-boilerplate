{
  "name": "ssr-react-next",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "start:app": "node app/server.js",
    "start:apis": "cd ./apis && sls offline && cd ..",
    "start": "concurrently --kill-others \"npm run start:app\" \"npm run start:apis\"",
    "deploy:app": "cd ./app && sls create_domain && sls deploy && cd ..",
    "deploy:apis": "cd ./apis && sls create_domain && sls deploy && cd ..",
    "deploy": "npm run deploy:apis && npm run deploy:app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "next": "^8.0.4",
    "path-match": "^1.2.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.7.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^3.2.1",
    "serverless-http": "^1.9.1",
    "serverless-nextjs-plugin": "^1.5.2",
    "serverless-offline": "^4.9.4",
    "serverless-s3-local": "^0.3.23",
    "webpack": "^4.29.6"
  }
}
