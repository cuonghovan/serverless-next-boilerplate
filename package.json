{
  "name": "ssr-react-next",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "start:app": "node app/server.js",
    "start:apis": "cd ./apis && sls offline && cd ..",
    "start": "concurrently --kill-others \"npm run start:app\" \"npm run start:apis\"",
    "deploy:app": "cd ./app && sls create_domain && sls deploy && cd ..",
    "deploy:apis": "cd ./apis && sls create_domain && sls deploy && cd ..",
    "deploy": "npm run deploy:apis && npm run deploy:app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "express": "^4.16.4",
    "next": "^8.0.4",
    "path-match": "^1.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "concurrently": "^4.1.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^3.2.1",
    "serverless-http": "^1.9.1",
    "serverless-nextjs-plugin": "^1.5.2",
    "serverless-offline": "^4.9.4",
    "serverless-s3-local": "^0.3.23",
    "webpack": "^4.29.6"
  }
}
